
public class Keksi {

	private String nimi;
	private int paino;
	private String valmistuspvm;

	public void setPaino(int uusiPaino) {
		if (uusiPaino > 0) {
			paino = uusiPaino;
		}
	}

	public int getPaino() {
		return paino;
	}

	public String toString() {
		return ("Nimi: " + nimi + " Paino: " + paino + " Valmistuspäivä: " + valmistuspvm);
	}
}
